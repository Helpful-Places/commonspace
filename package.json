{
  "name": "gehl-protocol-data-collector",
  "version": "0.0.1",
  "description": "digital collection of gehl public life study data",
  "main": "index.js",
  "author": "Sidewalk Labs Eng",
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn webpack",
    "build-scripts": "tsc -m commonJS --rootDir src --outDir js_scripts",
    "clean": "rm -rf dist/",
    "deploy-gcp": "npm run build && cp package.json dist/ && gcloud beta functions deploy addSurveyorToStudy --env-vars-file .env.yml --trigger-http --runtime nodejs8 --source dist/",
    "lint": "tsc --noEmit",
    "test": "jest",
    "watch": "webpack watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.56",
    "@babel/core": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-typescript": "^7.0.0-beta.55",
    "@types/jest": "^23.3.1",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "firebase-tools": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/pg": "^7.4.10",
    "@types/uuid": "^3.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "firebase": "^5.3.1",
    "firebase-admin": "^6.0.0",
    "pg": "^7.4.3",
    "pg-error-constants": "^1.0.0",
    "uuid": "^3.3.2"
  }
}
