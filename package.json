{
  "name": "gehl-protocol-data-collector",
  "version": "0.0.1",
  "description": "digital collection of gehl public life study data",
  "main": "index.js",
  "author": "Sidewalk Labs Eng",
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn webpack",
    "build-scripts": "tsc -m commonJS --rootDir src --outDir js_scripts",
    "build-gcp": "npm run build && cp package.json dist/",
    "clean": "rm -rf dist/",
    "deploy-gcp": "npm run build-gcp && npm run deploy-saveNewUser & npm run deploy-saveNewStudy & npm run deploy-saveNewSurvey & npm run deploy-addSurveyorToStudy & npm run deploy-saveNewLocation & npm run deploy-saveDataPointToStudy & npm run deploy-deleteDataPointFromStudy",
    "deploy-gcp-only-function": "npm run build-gcp && gcloud beta functions deploy --env-vars-file .env.yml --trigger-http --runtime nodejs8 --source dist/",
    "deploy-gcp-function": "gcloud beta functions deploy --env-vars-file .env.yml --trigger-http --runtime nodejs8 --source dist/",
    "deploy-saveNewUser": "npm run deploy-gcp-function saveNewUser",
    "deploy-saveNewStudy": "npm run deploy-gcp-function saveNewStudy",
    "deploy-saveNewSurvey": "npm run deploy-gcp-function saveNewSurvey",
    "deploy-addSurveyorToStudy": "npm run deploy-gcp-function addSurveyorToStudy",
    "deploy-saveNewLocation": "npm run deploy-gcp-function saveNewLocation",
    "deploy-saveDataPointToStudy": "npm run deploy-gcp-function saveDataPointToStudy",
    "deploy-deleteDataPointFromStudy": "npm run deploy-gcp-function deleteDataPointFromStudy",
    "lint": "tsc --noEmit",
    "it": "jest -c jest.config.integration.js --runInBand",
    "test": "jest -c jest.config.test.js",
    "watch": "webpack watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.56",
    "@babel/core": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0-beta.55",
    "@types/jest": "^23.3.1",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "firebase-tools": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.1.2",
    "@material-ui/icons": "^3.0.1",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/material-ui": "^0.21.5",
    "@types/pg": "^7.4.10",
    "@types/uuid": "^3.4.3",
    "csvtojson": "^2.0.8",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "firebase": "^5.5.2",
    "firebase-admin": "^6.0.0",
    "firebaseui": "^3.4.1",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8",
    "moment": "^2.22.2",
    "pg": "^7.4.3",
    "pg-error-constants": "^1.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "uuid": "^3.3.2"
  }
}
